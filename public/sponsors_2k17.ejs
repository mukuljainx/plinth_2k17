<!doctype html>
<html class="no-js" lang="en" dir="ltr">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Sponsors | Plinth'17</title>
         <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <script src="https://use.fontawesome.com/8720ae4a19.js"></script>
        <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.2.4/foundation.min.css" rel="stylesheet">
        <link rel="stylesheet" href="css/common.css">
        <link rel="stylesheet" href="css/sponsors_2k17.css" />
    </head>
    <body>

        <% include partials/header %>
        <%if (!isLoggedIn) { %>
                <% include partials/reg-form %>
        <% } %>

        <div class="content">
            <div class="row head">
                <div class="small-10 small-centered columns text-center">
                    <h4>Our Sponsors</h4>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="small-10 small-centered columns">
                <div class="spons-content-big">
                    <img src="media/sponsors_2k17/ei-logo.svg" alt="Engineer Indya" class="ei"/>
                    <div class="name-big">
                        <span>Engineer Indya</span>
                    </div>
                </div>
            </div>
        </div>

<!--        <div class="big-wrapper">-->
            <div class="row">
                <div class="small-10 small-centered columns">
                    <div class="row">
                        <div class="small-10 medium-4 large-4 columns">
                            <div class="spons-content">
                                <img src="media/sponsors_2k17/GrabOn.png" alt="GrabOn" class="grab"/>
                                <div class="name go">
                                    <span>Coupon Partner</span>
                                </div>
                            </div>
                        </div>
                        <div class="small-10 medium-4 large-4  columns">
                            <div class="spons-content">
                                <img src="media/sponsors_2k17/OYO_Logo.png" alt="OYO" class="oyo"/>
                                <div class="name oy">
                                    <span>Hospitality Partner</span>
                                </div>
                            </div>
                        </div>
                        <div class="small-10 medium-4 large-4 end columns" >
                            <div class="spons-content">
                                <img src="media/sponsors_2k17/zoom.jpg" alt="Zoom Car" class="zoom"/>
                                <div class="name zoo">
                                    <span>Travel Partner</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
<!--        </div>-->

      <% include partials/footer %>

    </body>
</html>
